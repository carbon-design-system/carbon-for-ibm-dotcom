{
  "name": "@carbon/ibmdotcom-react",
  "description": "Carbon for IBM.com React Components",
  "version": "1.45.0-rc.1",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": "https://github.com/carbon-design-system/carbon-for-ibm-dotcom",
  "bugs": "https://github.com/carbon-design-system/carbon-for-ibm-dotcom/issues",
  "files": [
    "lib/**/*",
    "es/**/*",
    "umd/**/*"
  ],
  "keywords": [
    "react",
    "carbon",
    "carbon-components",
    "ibm",
    "carbon-design-system",
    "components"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && gulp vendor && node scripts/build.js",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider BABEL_ENV=es build-storybook",
    "build-storybook:experimental": "cross-env DDS_FLAGS_ALL=true build-storybook -o storybook-static-experimental",
    "build-storybook:rtl": "cross-env STORYBOOK_USE_RTL=true build-storybook -o storybook-static-rtl",
    "ci-check": "yarn test-ssr && yarn test:unit",
    "clean": "rimraf src/internal/vendor es lib umd storybook-static",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "doctoc": "doctoc --title '## Table of Contents' docs",
    "preinstall": "echo \"⛔️⛔️⛔️ATTENTION: @carbon/ibmdotcom-react package will be deprecated in Carbon for IBM.com V2. This package will fall into maintaince-mode only; we will make updates only for potential security issues and high severity bugs. Head to https://www.ibm.com/standards/carbon/developing/frameworks/react for more details.⛔️⛔️⛔️\"",
    "postinstall": "carbon-telemetry collect --install",
    "start": "yarn storybook",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 9000",
    "test:e2e:build": "node ./tests/e2e/build/build-examples.js",
    "test:e2e:run": "NODE_OPTIONS=--openssl-legacy-provider npx -y http-server -c-1 tests/e2e/dist",
    "test:e2e:test": "NODE_OPTIONS=--openssl-legacy-provider npx -y start-server-and-test 'yarn test:e2e:run' 8080 'percy exec --config tests/e2e/.percy.json -- cypress run --config video=false --config-file tests/e2e/cypress.json'",
    "test:e2e:test:no-percy": "NODE_OPTIONS=--openssl-legacy-provider start-server-and-test 'yarn test:e2e:run' 8080 'cypress run --config video=false --config-file tests/e2e/cypress.json'",
    "test:e2e-storybook:run": "NODE_OPTIONS=--openssl-legacy-provider npx -y http-server -c-1 storybook-static --silent -p 8082",
    "test:e2e-storybook:test": "npx -y start-server-and-test 'yarn test:e2e-storybook:run' 8082 'percy exec --config tests/e2e-storybook/.percy.json -- cypress run --config video=false --config-file tests/e2e-storybook/cypress.json'",
    "test:e2e-storybook:dev": "cypress open --config-file ./tests/e2e-storybook/cypress-local.json",
    "test:e2e-storybook:test:debug": "npx -y start-server-and-test 'yarn test:e2e-storybook:run' 8082 'cypress open --config-file tests/e2e-storybook/cypress.json'",
    "test:e2e-storybook:test:no-percy": "npx -y start-server-and-test 'yarn test:e2e-storybook:run' http://0.0.0.0:8082 'cypress run --config-file tests/e2e-storybook/cypress.json'",
    "test:e2e-storybook:test:no-percy:firefox": "start-server-and-test 'yarn test:e2e-storybook:run' 8082 'cypress run --config-file tests/e2e-storybook/cypress.json --browser firefox'",
    "test:e2e-storybook:test:no-percy:with-video": "npx -y start-server-and-test 'yarn test:e2e-storybook:run' 8082 'cypress run --config video=true,screenshotOnRunFailure=true --config-file tests/e2e-storybook/cypress.json'",
    "test:e2e-storybook:browserstack:canary": "browserstack-cypress run --cf tests/e2e-storybook/browserstack.json --ccf tests/e2e-storybook/cypress-canary.json --sync --build-name 'React Storybook Canary'",
    "test:e2e-storybook:browserstack:next": "browserstack-cypress run --cf tests/e2e-storybook/browserstack.json --ccf tests/e2e-storybook/cypress-next.json --sync --build-name 'React Storybook Next'",
    "test:unit": "cross-env NODE_OPTIONS=--unhandled-rejections=none jest",
    "test:unit:updateSnapshot": "cross-env NODE_OPTIONS=--unhandled-rejections=none jest -u",
    "test:a11y": "gulp test:a11y || true",
    "test-ssr": "yarn build && node ssr-tests/*.js",
    "visual-snapshot": "yarn build-storybook && percy storybook ./storybook-static"
  },
