  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && gulp vendor:carbon-components-react && node scripts/build.js",
    "build-storybook": "cross-env BABEL_ENV=es build-storybook",
    "ci-check": "yarn test-ssr && yarn test:unit",
    "clean": "rimraf src/internal/vendor es lib umd storybook-static",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "doctoc": "doctoc --title '## Table of Contents' docs",
    "prepublish": "yarn build",
    "semantic-release": "semantic-release",
    "start": "yarn storybook",
    "storybook": "start-storybook -p 9000",
    "test": "yarn test:unit && yarn test:integration",
    "test:e2e": "npx wdio wdio.conf.js",
    "test:unit": "jest",
    "test:unit:updateSnapshot": "jest -u",
    "test:a11y": "gulp test:a11y",
    "test:integration": "yarn test:integration:build && yarn test:integration:bundle && yarn test:integration:ui",
    "test:integration:build": "jest -c tests/integration/build/jest.config.js --runInBand",
    "test:integration:bundle": "cross-env NODE_OPTIONS=--max-old-space-size=8192 jest -c tests/integration/bundle/jest.config.js",
    "test:integration:ui": "jest -c tests/integration/ui/jest.config.js --runInBand",
    "test-ssr": "yarn build && node ssr-tests/*.js",
    "upgrade-carbon": "yarn upgrade-interactive carbon-components carbon-components-react carbon-icons @carbon/icons-react @carbon/pictograms-react @carbon/storybook-addon-theme --latest --exact",
    "visual-snapshot": "yarn build-storybook && percy-storybook --widths=320,1280"
  },
