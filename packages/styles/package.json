{
  "name": "@carbon/ibmdotcom-styles",
  "description": "Carbon for IBM.com Styles",
  "version": "1.50.0",
  "license": "Apache-2.0",
  "main": "dist/ibm-dotcom-styles.min.css",
  "module": "src/scss",
  "repository": "https://github.com/carbon-design-system/carbon-for-ibm-dotcom",
  "bugs": "https://github.com/carbon-design-system/carbon-for-ibm-dotcom/issues",
  "files": [
    "dist/**/*",
    "scss/**/*",
    "icons/**/*"
  ],
  "keywords": [
    "react",
    "carbon",
    "carbon-components",
    "ibm",
    "carbon-design-system",
    "components"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "gulp",
    "ci-check": "yarn build",
    "postinstall": "carbon-telemetry collect --install",
    "start": "gulp dev",
    "upgrade-carbon": "yarn upgrade-interactive @carbon/grid @carbon/import-once @carbon/layout @carbon/motion @carbon/themes @carbon/type carbon-components @carbon/icons-react --latest --exact"
  },
  "dependencies": {
    "@carbon/grid": "10.43.1",
    "@carbon/icons-react": "10.49.1",
    "@carbon/import-once": "10.7.0",
    "@carbon/layout": "10.37.1",
    "@carbon/motion": "10.29.1",
    "@carbon/telemetry": "0.1.0",
    "@carbon/themes": "10.55.2",
    "@carbon/type": "10.45.2",
    "carbon-components": "10.58.8"
  },
  "devDependencies": {
    "carbon-components-react": "7.59.8",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-rtlcss": "^1.4.1",
    "gulp-sass": "^5.0.0",
    "node-sass": "^8.0.0",
    "require-dir": "^1.2.0"
  }
}
