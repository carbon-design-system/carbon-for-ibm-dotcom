{
  "parserOptions" : {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": [
    "plugin:lit/recommended",
    "../.eslintrc.js"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "class-methods-use-this": [2, { "exceptMethods": ["render"] }],
    "no-underscore-dangle": 0
  }
}
