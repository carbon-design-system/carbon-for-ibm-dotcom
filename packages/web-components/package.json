  "main": "es/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "exports": {
    "./es/": "./es/",
    "./custom-elements.json": "./custom-elements.json",
    "./package.json": "./package.json"
  },
  "files": [
    "es/**/*",
    "scss/**/*",
    "custom-elements.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/carbon-design-system/ibm-dotcom-library/tree/master/packages/web-components",
  "bugs": "https://github.com/carbon-design-system/ibm-dotcom-library/issues",
  "keywords": [
    "ibm",
    "carbon",
    "carbon-design-system",
    "pattern",
    "patterns",
    "style guide",
    "component",
    "components",
    "web components"
  ],
  "scripts": {
    "build": "gulp build && yarn wca",
    "build-storybook": "build-storybook",
    "ci-check": "yarn wca && yarn format:diff && yarn lint:src && yarn typecheck && yarn build && yarn test:unit",
    "clean": "rimraf es scss storybook-static custom-elements.json",
    "format": "prettier --write \"**/*.{css,js,ts,md,scss}\"",
    "format:diff": "prettier --check \"**/*.{css,js,ts,md,scss}\"",
    "format:staged": "prettier --write",
    "lint": "yarn lint:src",
    "lint:src": "yarn lint:scripts",
    "lint:scripts": "eslint --ext .js,.ts,.tsx .",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "gulp test:unit && yarn jest",
    "test:unit:updateSnapshot": "gulp test:unit --update-snapshot",
    "test:integration": "yarn test:integration:build && yarn test:integration:ui",
    "test:integration:build": "jest -c tests/integration/build/jest.config.js --runInBand",
    "test:integration:ui": "jest -c tests/integration/ui/jest.config.js --runInBand",
    "start": "yarn storybook",
    "storybook": "start-storybook -p 9000",
    "upgrade-carbon": "yarn upgrade-interactive carbon-components carbon-web-components @carbon/icon-helpers @carbon/icons --latest --exact",
    "wca": "web-component-analyzer analyze src --outFile custom-elements.json"
  },
  "resolutions": {
    "prettier": "^1.19.0"
  },
