{
  "extends": "./tsconfig-angular-esm2015.json",
  "compilerOptions": {
    "target": "es5",
    "outDir": "./es/directives-angular/esm5",
    "rootDir": "./src/directives-angular"
  },
  "angularCompilerOptions": {
    "annotateForClosureCompiler": true,
    "disableTypeScriptVersionCheck": true,
