{
  "lerna": "6.4.1",
  "version": "independent",
  "npmClient": "yarn",
  "npmClientArgs": ["--no-immutable"],
  "useWorkspaces": true,
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/docs/**",
    "**/e2e/**",
    "**/examples/**",
    "**/*.md",
    "**/ssr-tests/**"
  ],
  "command": {
    "version": {
      "conventionalCommits": true,
      "exact": true,
      "message": "chore(release): publish"
    }
  }
}
