{
  "lerna": "3.20.2",
  "version": "independent",
  "npmClientArgs": [
    "--frozen-lockfile"
  ],
  "useWorkspaces": true,
  "ignoreChanges": [
    "**/__fixtures__/**",
    "**/__tests__/**",
    "**/docs/**",
    "**/e2e/**",
    "**/examples/**",
    "**/*.md",
    "**/ssr-tests/**"
  ],
  "command": {
    "version": {
      "conventionalCommits": true,
      "exact": true,
      "message": "chore(release): publish"
    }
  }
}
